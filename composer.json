        "symfony/http-kernel": "~2.4, >2.4.6",
        "symfony/config": "~2.4",
        "symfony/dependency-injection": "~2.4",
        "mmoreram/simple-doctrine-mapping": "~0.1",
        "ircmaxell/password-compat": "~1.0",

        "elcodi/core-bundle": "~0.3.0",
        "elcodi/geo-bundle": "~0.3.0",
